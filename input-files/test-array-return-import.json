{
  "openapi": "3.0.3",
  "info": {
    "title": "Array Return Import Test",
    "version": "1.0.0"
  },
  "paths": {
    "/items": {
      "get": {
        "operationId": "listItems",
        "summary": "List Items",
        "tags": ["Items"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Item"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Item": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },
          "name": { "type": "string" }
        },
        "required": ["id", "name"]
      }
    }
  }
}
